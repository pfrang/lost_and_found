<h2>Report a pet as found!</h2>

<%= form_for @pet do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
  <%= f.label :species %>
  <%= f.select :species, ['Dog', 'Cat', 'Fish', 'Bird', 'Lizard'] %>
  <%= f.label :found_on %>
  <%= f.date_field :found_on %>
  <%= f.submit%>
<% end %>
